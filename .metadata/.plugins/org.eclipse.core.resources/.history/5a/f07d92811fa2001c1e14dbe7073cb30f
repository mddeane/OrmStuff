package com.revature.main;

import java.util.Arrays;
import java.util.List;

import com.revature.models.Account;
import com.revature.models.User;
import com.revature.util.Configuration;

public class App {

	public static void main(String[] args) {

		List<Class<?>> ormClasses = Arrays.asList(Account.class, User.class);

		Configuration cfg = new Configuration();

		Configuration annotatedClasses = cfg.addAnnotatedClass(ormClasses);

		System.out.println(annotatedClasses.toString());
		
		List<MetaModel<Class<?>>> ormMetaModels
		System.out.println(annotatedClasses.getMetaModels());


	}

}
